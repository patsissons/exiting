{
  "name": "exiting",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "pnpm": "^10",
    "node": "^22"
  },
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "conf": "pnpm run conf:link && pnpm run conf:env",
    "conf:link": "test -f .vercel/project.json || vercel link",
    "conf:env": "./scripts/setup-env.sh",
    "conf:clean": "rm -rf .vercel .env*.local",
    "db": "pnpm run db:start",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:status": "supabase status",
    "db:reset": "supabase db reset",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:stats": "pnpm lint:ts --format node_modules/eslint-stats/byError.js",
    "pretty": "prettier --write '**/*.{css,scss,js,json,md,ts,tsx,yml}'",
    "validate": "tsc --noEmit"
  },
  "dependencies": {
    "@shopify/polaris": "^10.12.0",
    "@supabase/supabase-js": "2.1.0",
    "@uiw/react-markdown-editor": "^5.10.0",
    "@uiw/react-markdown-preview": "^4.1.5",
    "moment": "^2.29.4",
    "next": "13.0.4",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@shopify/typescript-configs": "^5.1.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.4",
    "eslint-stats": "^1.0.1",
    "next-remove-imports": "^1.0.8",
    "typescript": "4.9.3",
    "vercel": "^28.5.5"
  },
  "optionalDependencies": {
    "supabase": "1.26.9"
  }
}
